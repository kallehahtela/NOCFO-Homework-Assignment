{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "module": "ESNext",
    "esModuleInterop": true,
    "allowJs": true,
    "target": "ES2024",
    "removeComments": false,
  }
}
